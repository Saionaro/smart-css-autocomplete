{
  "name": "smart-css-autocomplete",
  "displayName": "Smart CSS Autocomplete",
  "description": "Autocomplete CSS properties smarter - using an usage statistics instead of standard random",
  "license": "MIT",
  "version": "1.0.0",
  "categories": [],
  "author": "Saionaro <shibakow@gmail.com>",
  "publisher": "saionaro",
  "repository": {
    "type": "git",
    "url": "https://github.com/Saionaro/smart-css-autocomplete.git"
  },
  "bugs": {
    "url": "https://github.com/Saionaro/smart-css-autocomplete/issues"
  },
  "keywords": [
    "autocomplete",
    "suggestions",
    "css",
    "less",
    "scss",
    "props",
    "properties"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:less"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "typecheck": "tsc --noEmit",
    "watch": "tsc -b -w",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write"
  },
  "dependencies": {
    "known-css-properties": "^0.19.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.2",
    "@types/node": "^12.12.0",
    "@types/vscode": "1.43.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  }
}
